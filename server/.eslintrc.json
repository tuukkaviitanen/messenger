{
    "env": {
        "es2021": true,
        "node": true,
        "jest/globals": true,
        "cypress/globals": true
    },
    "extends": "xo",
    "overrides": [
        {
            "extends": [
                "xo-typescript"
            ],
            "files": [
                "*.ts",
                "*.tsx"
            ]
        }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["unused-imports", "jest", "cypress"],
    "rules": {
        "no-console":"error",
        "unused-imports/no-unused-imports": "error"
    }
}
